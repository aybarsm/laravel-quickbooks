{
    "name": "aybarsm/laravel-quickbooks",
    "description": "A Laravel Service Provider Package for QuickBooks Online API",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "aybarsm",
        "Laravel",
        "php",
        "QuickBooks",
        "intuit"
    ],
    "homepage": "https://github.com/aybarsm/laravel-quickbooks",
    "authors": [
        {
            "name": "Murat Aybars",
            "email": "hello@aybarsm.com"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "aybarsm/laravel-quickbooks-dev": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Aybarsm\\Laravel\\QuickBooks\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aybarsm\\Laravel\\QuickBooks\\QuickBooksServiceProvider"
            ],
            "aliases": {
                "QuickBooks": "Aybarsm\\Laravel\\QuickBooks\\Facades\\QuickBooks",
                "QuickBooksHelper": "Aybarsm\\Laravel\\QuickBooks\\Services\\QuickBooksHelper"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
